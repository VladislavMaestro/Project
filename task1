spisok = ["a", "1.0", "-2", "1.2"]
i = 0
x = len(spisok)
while i < x: 
    if spisok[i].isalpha() == True: 
        spisok.remove(spisok[i]) 
        x -= 1 
        if float(spisok[i])%2 != 0: 
            spisok.remove(spisok[i]) 
            x -= 1 
            i+=1
            print(spisok)
